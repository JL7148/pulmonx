        <!-- content -->
    <main id="content" class="tf_box tf_clearfix">
	                <div id="page-80101" class="type-page">
                            <div class="page-content entry-content">
            <!--themify_builder_content-->
<div id="themify_builder_content-80101" data-postid="80101" class="themify_builder_content themify_builder_content-80101 themify_builder tf_clear">
                    <div  data-lazy="1" class="module_row themify_builder_row tb_4op2704 tf_w">
                        <div class="row_inner col_align_top tb_col_count_1 tf_box tf_rel">
                        <div  data-lazy="1" class="module_column tb-column col-full tb_bysw704 first">
                    <!-- module text -->
<div  class="module module-text tb_7618705    hide-tablet hide-mobile" data-lazy="1">
        <div  class="tb_text_wrap">
        <h1>Get Back to Living Life on Your Terms With the Zephyr<sup style="font-size: 50%;">®</sup> Valve</h1>    </div>
</div>
<!-- /module text -->        <div  data-lazy="1" class="module_subrow themify_builder_sub_row tf_w col_align_middle tb_col_count_2 tb_dbbs704 hide-tablet hide-mobile">
                <div  data-lazy="1" class="module_column sub_column tb_v1kx704 first">
                    <!-- module text -->
<div  class="module module-text tb_g9te179   " data-lazy="1">
        <div  class="tb_text_wrap">
        <h4 style="color: #2d95b4;">The Zephyr Valve is a minimally invasive and clinically proven treatment that is not another medication or surgery. If you have severe COPD/emphysema, this option may be right for you.</h4>    </div>
</div>
<!-- /module text -->        </div>
                    <div  data-lazy="1" class="module_column sub_column tb_eoxa705 last">
                    <!-- module icon -->
